{
  "name": "presta",
  "version": "0.16.0",
  "description": "The simple way to build the modern web.",
  "main": "index.js",
  "bin": {
    "presta": "presta.js"
  },
  "scripts": {
    "test": "npm run fixtures && NODE_ENV=test node -r esm test && npm run fixtures:remove",
    "test:watch": "npm run fixtures && NODE_ENV=test nodemon -i ./fixtures -r esm test",
    "fixtures": "node test/fixtures",
    "fixtures:remove": "rm -rf /fixtures",
    "format": "prettier-standard --format",
    "lint": "prettier-standard --lint",
    "cloc": "npx cloc lib/*.js cli.js presta.js document.js index.js load.js render.js serve.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier-standard --format --staged && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sure-thing/presta.git"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sure-thing/presta/issues"
  },
  "homepage": "https://github.com/sure-thing/presta#readme",
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "ansi-colors": "^4.1.1",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "cosmiconfig": "^7.0.0",
    "debug": "^4.1.1",
    "deepmerge": "^4.2.2",
    "esm": "^3.2.25",
    "exit": "^0.1.2",
    "finalhandler": "^1.1.2",
    "flat-cache": "^2.0.1",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "ignore": "^5.1.8",
    "matched": "^5.0.0",
    "module-alias": "^2.2.2",
    "ms": "^2.1.2",
    "p-queue": "^6.6.1",
    "pocket.io": "^0.1.4",
    "sade": "^1.7.3",
    "serve-static": "^1.14.1",
    "watch-dependency-graph": "^1.0.0",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "baretest": "^2.0.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.4",
    "prettier-standard": "^16.4.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.3"
  }
}
